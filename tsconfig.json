{
  "compilerOptions": {
    "baseUrl": ".",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es2021",
    "lib": ["es2021"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitReturns": true,
    "strictFunctionTypes": true,
    "types": ["@types/node"]
  },
}
